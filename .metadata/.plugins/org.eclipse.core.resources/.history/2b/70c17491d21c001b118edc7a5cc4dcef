package Framework;

import org.testng.annotations.Test;

public class Sample {
	@Test(description = "Print X", "priority = 3)
	public void x() {
		System.out.println("X");
	}

	@Test(description = "Print Y", priority = 2)
	public void y() {
		System.out.println("Y");
	}

	@Test(description = "Print Z", priority = 1)
	public void z() {
		System.out.println("Z");
	}

}
